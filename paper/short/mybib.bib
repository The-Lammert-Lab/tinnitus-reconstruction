  shorttitle = {Clinical {{Practice Guideline}}},
  author     = {Tunkel, David E. and Bauer, Carol A. and Sun, Gordon H. and Rosenfeld, Richard M. and Chandrasekhar, Sujana S. and Cunningham, Eugene R. and Archer, Sanford M. and Blakley, Brian W. and Carter, John M. and Granieri, Evelyn C. and Henry, James A. and Hollingsworth, Deena and Khan, Fawad A. and Mitchell, Scott and Monfared, Ashkan and Newman, Craig W. and Omole, Folashade S. and Phillips, C. Douglas and Robinson, Shannon K. and Taw, Malcolm B. and Tyler, Richard S. and Waguespack, Richard and Whamond, Elizabeth J.},
